{
  "name": "dan/aicrawler",
  "description": "A web scraping pattern combining heuristics with the Symfony DOMCrawler.",
  "license": "MIT",
  "keywords": ["scraper","crawler","ai"],
  "authors": [
    {
      "name": "Dan Richards",
      "email": "danrichardsri@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "symfony/dom-crawler": "2.7.*",
    "symfony/css-selector": "2.7.*",
    "dan/core": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8@dev"
  },
  "autoload": {
    "psr-4": {
      "Dan\\AiCrawler\\": "src/Dan/AiCrawler/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AiCrawlerTests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
